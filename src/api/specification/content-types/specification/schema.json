        "Qty Per Pallet",
        "Dimensions LxWxT",
        "Weight",
        "Performance Warranty",
        "Product Warranty",
        "Product Series",
        "Power Rating",
        "Inverter Type",
        "Phase Support",
        "Plug Connector Type",
        "Grid Support",
        "IP Rating",
        "Number Of MPPTs",
        "Number Of Strings",
        "Length",
        "Thickness",
        "Total Capacity",
        "Battery Voltage",
        "Battery Cell Technology",
        "Number of Battery Cells",
        "Max System Paralleled",
        "Dimensions WxHxD"
      ],
      "required": true,
      "default": "Brand"
