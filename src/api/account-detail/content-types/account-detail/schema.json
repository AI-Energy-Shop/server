        "SMALL",
        "MID-SIZED",
        "VIP"
      ]
    },
    "user_type": {
      "type": "enumeration",
      "enum": [
        "INSTALLER",
        "RETAILER",
        "ADMINISTRATOR",
        "SALES"
      ]
    },
    "odoo_user_id": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "first_name": {
      "type": "string"
    },
    "middle_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "business_name": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "shipping_addresses": {
      "displayName": "Address",
      "type": "component",
      "repeatable": true,
      "component": "elements.address"
    },
    "payment_options": {
      "displayName": "Payment Option",
      "type": "component",
      "repeatable": true,
      "component": "elements.payment-option"
    }
