{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users"
    },
    "account_detail": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::account-detail.account-detail",
      "inversedBy": "user"
    },
    "account_status": {
      "type": "enumeration",
      "enum": [
        "PENDING",
        "REVIEWING",
        "APPROVED",
        "DENIED"
      ]
    },
    "user_notifications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-notification.user-notification",
      "mappedBy": "user"
    },
    "carts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cart.cart",
      "mappedBy": "user"
    },
    "business_name": {
      "type": "string"
    },
    "business_number": {
      "type": "string"
    },
    "business_type": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "addresses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::address.address",
      "mappedBy": "users"
    },
    "createAccountRequest": {
      "type": "datetime"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "accountCredit": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::account-credit.account-credit",
      "inversedBy": "user"
    },
    "creditCards": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::credit-card.credit-card"
    },
    "stripeCustomerID": {
      "type": "string"
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order"
    }
  }
}
